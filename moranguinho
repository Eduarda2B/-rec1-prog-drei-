    <script>
  (function() {
    var sendMetrics = function() {
      try {
        var url = new URL('https://analytics.awsli.com.br/log.php');
        url.searchParams.set('store_id', 129579);
        url.searchParams.set('device_type', mobileCheck() ? 'mobile' : 'desktop');
        url.searchParams.set('page', 'product');
        if (window.performance) {
          var navigationTimings = performance.getEntriesByType('navigation');
          if (navigationTimings.length) {
            var navigationTiming = navigationTimings[0];
            // https://www.w3.org/TR/navigation-timing-2/timestamp-diagram.svg
            var connection_time = Math.round(navigationTiming.requestStart - navigationTiming.startTime)
            var server_response_time = Math.round(navigationTiming.responseStart - navigationTiming.requestStart)
            var total_response_time = Math.round(navigationTiming.responseEnd - navigationTiming.requestStart)
            var render_time = Math.round(navigationTiming.loadEventEnd - navigationTiming.domInteractive)
            url.searchParams.set('connection_time', connection_time);
            url.searchParams.set('server_response_time', server_response_time);
            url.searchParams.set('total_response_time', total_response_time);
            url.searchParams.set('render_time', render_time);
          };
        };
        (navigator.sendBeacon && navigator.sendBeacon(url)) ||
          fetch(url, {method: 'POST', keepalive: true});
      } catch (err) { }
    }
