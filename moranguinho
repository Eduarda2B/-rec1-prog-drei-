<script type="text/javascript">
  $(function() {
    
      iniciarModalNews();
    
  });
  function iniciarModalNews() {
    if (!$.cookie('showModalNews')) {
      
        setTimeout(function(){
      
      
              showModalNews();
      
      
        }, 10000);
      
    };
  }
  function showModalNews() {
    $.fancybox.open({
      type: 'html',
      minWidth: 270,
      maxWidth: 350,
      content: $('#modalNewsletter'),
      beforeClose: function() {
        $.cookie('showModalNews', 'hide', {
          expires: 1,
          path: '/'
        });
      }
    });
  }
</script>


      
    

    
