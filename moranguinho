    <script>
  $(function() {
    // Clicar em um produto
    $('.listagem-item').click(function() {
      $(document).trigger('li_select_product', [{
        item_id: $(this).attr('data-id'),
        item_name: $(this).find('.nome-produto').text()
      }]);
    });

    // Clicar no WhatsApp
    $('.li-whatsapp a').click(function() {
      $(document).trigger('li_start_contact', ['WhatsApp']);
    });

    
      // Visualizar o produto
      $(document).trigger('li_view_product', [{
        item_id: '7267948',
        item_name: 'MORANGUINHO 002 A4',
        price: 7.50,
        quantity: 1
      }]);

      // Selecionar uma variação
      $('.atributo-item').click(function() {
        const $attribute = $(this);

        $(document).trigger('li_select_variation', [{
          grid_id: $attribute.data('grade-id'),
          variation_id: $attribute.data('variacao-id')
        }]);
      });

      // Calcular frete
      $('#formCalcularCep').submit(function() {
        const formData = new FormData(this);

        $(document).trigger('li_calculate_shipping', [formData.get('cep')]);
      });
    
  });
</script>

    


  
  
